{
  "name": "@thebund1st/daming-ui",
  "version": "0.3.25",
  "private": false,
  "main": "lib/index.js",
  "types": "type/index.d.ts",
  "module": "es/index.js",
  "scripts": {
    "example": "cross-env NODE_ENV=development webpack-dev-server --open --config config/webpack.dev.config.js --progress",
    "start": "cross-env NODE_ENV=debugger webpack-dev-server --open --config config/webpack.dev.config.js --progress",
    "build": "gulp -f ./build/gulpfile.js build",
    "format": "prettier --write \"example/**/*.ts\" \"example/**/*.tsx\" \"example/**/*.js\" \"example/**/*.jsx\" \"src/**/*.ts\" \"src/**/*.tsx\"",
    "e2e": "node example/test/e2e/runner.js",
    "test:src": "cross-env NODE_ENV=debugger yarn e2e",
    "test:lib": "cross-env NODE_ENV=development yarn e2e",
    "prepublishOnly": "yarn build && yarn test:lib"
  },
  "dependencies": {
    "antd": "^3.21.4",
    "lodash.clonedeep": "^4.5.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "@types/node": "^12.7.2",
    "@types/react": "^16.8.24",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.4",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "axios": "^0.18.0",
    "babel-register": "^6.26.0",
    "chromedriver": "^76.0.0",
    "cross-env": "^5.2.0",
    "cross-spawn": "^6.0.5",
    "css-loader": "^3.1.0",
    "eslint": "^6.1.0",
    "file-loader": "^4.1.0",
    "fork-ts-checker-notifier-webpack-plugin": "^1.0.2",
